work/vue-todo-list/client/src/components/addTodoItem.vue","sourceRoot":"","sourcesContent":["<template>\n\t<div class=\"addNewTodo\">\n\t\t<input type=\"text\" placeholder=\"New todo\" v-model=\"newTodosField\">\n\t\t<button @click = 'addNewTodos'>Add</button>\n\t\t<h5 class='error' v-if ='error.length > 0'>{{error}}</h5>\n\t</div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default{\n\tname:'addTodoItem',\n\tdata(){\n\t\treturn{\n\t\t\tnewTodosField:'',\n\t\t\terror:''\n\t\t};\n\t},\n\tprops:['todos'],\n\tmethods:{\n\t\taddNewTodos(){\n\t\t\tif (this.newTodosField.length > 3){\n\t\t\t\tconst path = 'http://localhost:5000/addNewTodos';\n\t\t\t\taxios.post(path, JSON.stringify(this.newTodosField))\n\t\t\t\t.then(res => {\n\t\t\t\t\tif (typeof res.data == 'string'){\n\t\t\t\t\t\tthis.error = res.data;\n\t\t\t\t\t}else if(res.data['new_id']){\n\t\t\t\t\t\tlet newItem = {\n\t\t\t\t\t\t\tid:res.data['new_id'],\n\t\t\t\t\t\t\tname: this.newTodosField.trim(),\n\t\t\t\t\t\t\tsuccessful:false\n\t\t\t\t\t\t};\n\t\t\t\t\t\tthis.$emit('add-new-todo', newItem);\n\t\t\t\t\t\tthis.newTodosField = '';\n\t\t\t\t\t\tthis.error = '';\n\t\t\t\t\t}else{\n\t\t\t\t\t\tthis.error = res.data['error'];\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch(res=>{\n\t\t\t\t\tconsole.log(res);\n\t\t\t\t})\n\t\t\t}else{\n\t\t\t\tthis.error = 'Write more text';\n\t\t\t}\n\t\t}\n\t}\n}\n</script>\n\n<style>\n\t.addNewTodo{\n\t\tmax-width: 300px;\n\t\tdisplay: flex;\n\t\tjustify-content: space-between;\n\t\tflex-wrap: wrap;\n\t\tmargin:0 auto 0;\n\t}\n\t.addNewTodo .error{\n\t\tmargin:5px 0 0;\n\t}\n</style>"]}]}