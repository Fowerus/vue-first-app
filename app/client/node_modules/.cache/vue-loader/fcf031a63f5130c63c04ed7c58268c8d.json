\t\ttitle:this.title,\n\t\t\t\t\ttext:this.text,\n\t\t\t\t\tauthor: localStorage.getItem('current_user')\n\t\t\t\t};\n\t\t\t\taxios.post(path, JSON.stringify(newPost))\n\t\t\t\t.then(res =>{\n\t\t\t\t\tif (typeof res.data == 'string'){\n\t\t\t\t\t\tthis.error = res.data;\n\t\t\t\t\t}else{\n\t\t\t\t\t\tnewPost['id'] = res.data;\n\t\t\t\t\t\tthis.error = 'Post added';\n\t\t\t\t\t\tthis.$emit('add-new-post', newPost);\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t}else{\n\t\t\t\tthis.error = 'Your inputs is short';\n\t\t\t}\n\t\t},\n\t\tcloseField(){\n\t\t\tthis.$emit('close-field');\n\t\t}\n\t},\n\tcomputed:{\n\t}\n}\n</script>\n\n<style scroped>\n\t.addPostItem{\n\t\tmax-width: 500px;\n\t\tmax-height: 700px;\n\t\twidth:100%;\n\t\theight:100%;\n\t\t\n\t\tborder:1px solid black;\n\t\tbackground: white;\n\t\tpadding:10px;\n\t\tmargin:0px auto 50px;\n\t}\n\t.addPostItem .closeField{\n\t\tcursor:pointer;\n\t\t-moz-user-select: none;\n\t\t-webkit-user-select: none;\n\t\t-ms-user-select: none;\n\t\tuser-select: none;\n\t\tmargin:0;\n\n\t\tposition: absolute;\n\t}\n\t.addPostItem form{\n\t\tmax-width: inherit;\n\t\tmargin:30px auto 0px;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t}\n\t.addPostItem form h2{\n\t\tmargin:0 0 20px 0;\n\t}\n\t.addPostItem form textarea, input:not(:last-child){\n\t\tmax-width: 400px;\n\t\twidth:100%;\n\t\theight:40px;\n\t\tfont-size: 15px;\n\t\tpadding:0 20px 0 20px;\n\t\tmargin:0 auto 10px;\n\t}\n\t.addPostItem form textarea{\n\t\tresize:vertical;\n\t\tmin-height: 150px;\n\t\tmax-height: 400px;\n\t\tfont-size: 13px;\n\t\tpadding:10px 20px 10px 20px;\n\t}\n\t.addPostItem form input:last-child{\n\t\tcursor:pointer;\n\t\tfont-size: 15px;\n\t\twidth:100px;\n\t\tpadding:5px 10px 5px 10px;\n\t\tmargin:0 0 10px 32px;\n\t}\n\t.addPostItem .error{\n\t\tmargin:0;\n\t}\n</style>\n"]}]}