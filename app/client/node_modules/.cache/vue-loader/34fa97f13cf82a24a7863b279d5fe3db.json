t":"","sourcesContent":["<template>\n\t<div class='addPostItem'>\n\t\t<h3 class='closeField' @click = 'closeField'>X</h3>\n\t\t<form>\n\t\t\t<h2>Create a new post</h2>\n\t\t\t<input type=\"text\" placeholder=\"title\" v-model='title'>\n\t\t\t<textarea placeholder=\"text\" maxlength='10000' v-model='text' cols = '25' wrap=\"soft | hard\"></textarea>\n\t\t\t<input @click = 'addPost' type=\"button\" value=\"Create\">\n\t\t</form>\n\t\t<h5 class='error' v-if='error.length > 0'>{{error}}</h5>\n\t</div>\n</template>\n\n<script>\nimport axios from 'axios'\t\n\nexport default{\n\tname:'postItem',\n\tdata(){\n\t\treturn{\n\t\t\ttitle:'',\n\t\t\ttext:'',\n\t\t\terror:''\n\t\t};\n\t},\n\tmethods:{\n\t\taddPost(){\n\t\t\tif (this.title.length > 7 && this.text.length > 20){\n\t\t\t\tconst path = 'http://localhost:5000/addNewPost';\n\t\t\t\tlet newPost = {\n\t\t\t\t\ttitle:this.title,\n\t\t\t\t\ttext:this.text,\n\t\t\t\t\tauthor: localStorage.getItem('current_user')\n\t\t\t\t};\n\t\t\t\taxios.post(path, JSON.stringify(newPost))\n\t\t\t\t.then(res =>{\n\t\t\t\t\tif (typeof res.data == 'string'){\n\t\t\t\t\t\tthis.error = res.data;\n\t\t\t\t\t}else{\n\t\t\t\t\t\tnewPost['id'] = res.data;\n\t\t\t\t\t\tthis.error = 'Post added';\n\t\t\t\t\t\tthis.$emit('add-new-post', newPost);\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t}else{\n\t\t\t\tthis.error = 'Your inputs is short';\n\t\t\t}\n\t\t},\n\t\tcloseField(){\n\t\t\tthis.$emit('close-field');\n\t\t}\n\t},\n\tcomputed:{\n\t}\n}\n</script>\n\n<style scroped>\n\t.addPostItem{\n\t\tmax-width: 500px;\n\t\tmax-height: 700px;\n\t\twidth:100%;\n\t\theight:100%;\n\t\t\n\t\tborder:1px solid black;\n\t\tbackground: white;\n\t\tpadding:10px;\n\t\tmargin:0px auto 50px;\n\t}\n\t.addPostItem .closeField{\n\t\tcursor:pointer;\n\t\t-moz-user-select: none;\n\t\t-webkit-user-select: none;\n\t\t-ms-user-select: none;\n\t\tuser-select: none;\n\t\tmargin:0;\n\n\t\tposition: absolute;\n\t}\n\t.addPostItem form{\n\t\tmax-width: inherit;\n\t\tmargin:30px auto 0px;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t}\n\t.addPostItem form h2{\n\t\tmargin:0 0 20px 0;\n\t}\n\t.addPostItem form textarea, input:not(:last-child){\n\t\tmax-width: 400px;\n\t\twidth:100%;\n\t\theight:40px;\n\t\tfont-size: 15px;\n\t\tpadding:0 20px 0 20px;\n\t\tmargin:0 auto 10px;\n\t}\n\t.addPostItem form textarea{\n\t\tresize:vertical;\n\t\tmin-height: 150px;\n\t\tmax-height: 400px;\n\t\tfont-size: 13px;\n\t\tpadding:10px 20px 10px 20px;\n\t}\n\t.addPostItem form input:last-child{\n\t\tcursor:pointer;\n\t\tfont-size: 15px;\n\t\twidth:100px;\n\t\tpadding:5px 10px 5px 10px;\n\t\tmargin:0 0 10px 32px;\n\t}\n\t.addPostItem .error{\n\t\tmargin:0;\n\t}\n</style>\n"]}]}